{
  "name": "@crumblecracker/cc",
  "version": "0.1.1",
  "description": "TypeScript bindings for the cc virtualization library",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinyrange/cc.git",
    "directory": "bindings/nodejs"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "virtualization",
    "vm",
    "container",
    "oci",
    "docker"
  ],
  "author": "",
  "license": "GPL-3.0-only",
  "engines": {
    "node": ">=18.0.0"
  },
  "optionalDependencies": {
    "@crumblecracker/cc-darwin-arm64": "0.1.0",
    "@crumblecracker/cc-darwin-x64": "0.1.0",
    "@crumblecracker/cc-linux-x64": "0.1.0",
    "@crumblecracker/cc-linux-arm64": "0.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsdown": "^0.20.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
